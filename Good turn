#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	for(int i=0;i<t; i++)
    {	int x,y;
	cin>>x; //no chef gets
	cin>>y; //no chefina gets
    if (x+y>6)
    {
        cout<<"YES"<<endl;
    }
    else
    {
        cout<<"NO"<<endl;
    }
    }
	return 0;
}
