#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	for(int i=0;i<t;i++)
	{
	    int x,y;
	    cin>>x>>y;
	    int p=x*100;
	    int q=y*10;
	    if(p>=q)
	    {
	        cout<<"cloth"<<endl;
	    }
	    else
	    {
	        cout<<"disposable"<<endl;
	    }
	}
	return 0;
}
