#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	for(int i=0;i<t;i++)
	{
	    int x;
	    cin>>x;
	    if(x<=3)
	    {
	        cout<<"bronze"<<endl;
	    }
	    else if(x>3 && x<=6)
	    {
	        cout<<"silver"<<endl;
	    }
	    else
	    {
	        cout<<"gold"<<endl;
	    }
	}
	return 0;
}
