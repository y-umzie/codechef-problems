#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,x;
	cin>>t;
	for(int i=0;i<t;i++)
	{
	    cin>>x;
	    if(x<4)
	    {
	        cout<<"MILD"<<endl;
	    }
	    else if(x>7)
	    {
	        cout<<"HOT"<<endl;
	    }
	    else //if(x>7)
	    {
	        cout<<"MEDIUM"<<endl;
	    }
	}
	return 0;
}
