#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,x,y;
	cin>>t;
	for(int i=0;i<t;i++)
	{
	    cin>>x>>y;
	    int choco=x*2;
	    int candy=y*5;
	    if(choco>candy)
	    {
	        cout<<"chocolate"<<endl;
	    }
	    else if(choco==candy)
	    {
	        cout<<"either"<<endl;
	    }
	    else
	    {
	        cout<<"candy"<<endl;
	    }
	}
	return 0;
}
