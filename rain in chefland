#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,x;
	cin>>t;
	for(int i=0;i<t;i++)
	{
	    cin>>x;
	    if(x>=7)
	    {
	        cout<<"heavy"<<endl;
	    }
	    else if(x<3)
	    {
	        cout<<"light"<<endl;
	    }
	    else 
	    {
	       cout<<"moderate"<<endl;
	    }
	}
	return 0;
}
