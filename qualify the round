#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	for(int i=0;i<t;i++)
	{
	    int x,a,b;
	    cin>>x>>a>>b;
	    if((a*1+b*2)>=x)
	    {
	        cout<<"qualify"<<endl;
	    }
	    else
	    {
	        cout<<"not qualify"<<endl;
	    }
	}
	return 0;
}
