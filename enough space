#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,n,x,y;
	//n= free space,x = files of size 1gb and y=files of size 2gb
	cin>>t;
	for(int i=0;i<t;i++)
	{
	    cin>>n>>x>>y;
	    int total=(1*x)+(2*y);
	    if(total<=n)
	    {
	        cout<<"yes"<<endl;
	    }
	    else
	    {
	        cout<<"no"<<endl;
	    }
	}
	return 0;
}
